}

.slider {
    flex-shrink: 1;
    font-size: 5%;
    color: rgb(255, 0, 0);
    -unity-font: url('project://database/Assets/GUI%20PRO%20Kit%20-%20Sci-Fi/ResourcesData/Sprite/Component/Fonts/Data%20Control%20by%20Vic%20Fieger/data-latin.ttf?fileID=12800000&guid=6d532a68516819b4180abeab0c210b7d&type=3#data-latin');
    -unity-font-definition: none;
    -unity-font-style: normal;
    max-width: 50%;
}

.unity-toggle {
    min-height: 100%;
    max-height: 100%;
    height: 100%;
}
